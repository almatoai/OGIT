			[ ogit:affects  ogit.MARS:Resource ]
			[ ogit:affects  ogit.MARS:Software ]
			[ ogit:affects  ogit.MARS:Machine ]
			[ ogit:affects  ogit.Automation:MARSNode ]
	);
