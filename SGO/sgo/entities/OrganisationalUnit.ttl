@prefix ogit:					<http://www.purl.org/ogit/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit:OrganisationalUnit
	a rdfs:Class;
	rdfs:subClassOf ogit:Entity;
	rdfs:label "OrganisationalUnit";
	dcterms:description "An entity that represents a organisational unit. E.g. an institution of a politcal party.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
    dcterms:valid       "start=2024-12-17;";
    dcterms:creator     "Semih Can Sancar";
	ogit:scope "SGO";
	ogit:parent ogit:Node;
	ogit:any-attributes "true";
	ogit:mandatory-attributes (
		ogit:name
	);
.
