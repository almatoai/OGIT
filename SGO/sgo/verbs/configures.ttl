	dcterms:modified "2017-12-15";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:history (
		[
			dcterms:identifier "1";
			dcterms:date "2015-04-27";
			dcterms:description "intitial inception";
			dcterms:creator "cwalker@arago.de";
		]
		[
			dcterms:identifier "2";
			dcterms:date "2017-12-15";
			dcterms:description "add organization";
			dcterms:creator "bneal@arago.de";
		]
	);
	ogit:allowed (
		[
			ogit:from ogit.Forum:Configuration;
			ogit:to ogit.Forum:Profile;
